{
	"$schema": "https://biomejs.dev/schemas/1.5.3/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"suspicious": {
				"noCommentText": "off"
			},
			"style": {
				"noUnusedTemplateLiteral": "off",
				"noUselessElse": "off",
				"noForEach": "warn"
			}
		}
	},
	"javascript": {
		"formatter": {
			"arrowParentheses": "always",
			"quoteStyle": "single",
			"semicolons": "always",
			"trailingComma": "es5",
			"quoteProperties": "asNeeded",
			"bracketSpacing": true,
			"bracketSameLine": false
		}
	}
}